#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/marcuswang/Desktop/yourbusiness-blog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/marcuswang/Desktop/yourbusiness-blog/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+autoprefixer@10.4.16_postcss@8.4.31/node_modules/autoprefixer/bin/autoprefixer" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+autoprefixer@10.4.16_postcss@8.4.31/node_modules/autoprefixer/bin/autoprefixer" "$@"
fi
